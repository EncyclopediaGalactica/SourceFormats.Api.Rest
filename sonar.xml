<?xml version="1.0" encoding="utf-8" ?>
<SonarQubeAnalysisProperties xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
    <Property Name="sonar.sources">src</Property>
    <Property Name="sonar.exclusions">
        **/bin/**/*,
        **/obj/**/*,
        **/*.dll
    </Property>
    <Property Name="sonar.coverage.exclusions">
        **/bin/**/*,
        **/obj/**/*,
        **/*.dll
    </Property>
    <Property Name="sonar.cs.dotcover.reportsPaths">
        **/*dotCover.Output.html
    </Property>
    <Property Name="sonar.cs.xunit.reportsPaths">
        **/*.trx
    </Property>
</SonarQubeAnalysisProperties>